#!/bin/sh
xrandr --output HDMI-0 --off --output DP-0 -r 144.00 --mode 2560x1440 --pos 3840x0 --rotate right --output DP-1 --off --output DP-2 -r 169.83 --primary --mode 2560x1440 --pos 5280x167 --rotate normal --output DP-3 --off --output DP-4 -r 60.00 --mode 3840x2160 --pos 0x0 --rotate normal --output DP-5 --off
