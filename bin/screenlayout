 !/bin/sh
xrandr --output HDMI-0 --off --output DP-0 -r 120.00 --mode 2560x1440 --pos 1920x0 --rotate right --output DP-1 --off --output DP-2 -r 119.88 --mode 2560x1440 --pos 3360x96 --rotate normal --output DP-3 --off --output DP-4 -r 60.00 --mode 1920x1080 --pos 0x360 --rotate normal --output DP-5 --off
